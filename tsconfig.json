{
  "compilerOptions": {
    "baseUrl": ".",
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "ESNext",
    "target": "ES2018",
    "moduleResolution": "Bundler",
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "lib": [
      "DOM",
      "ES2018"
    ],
    "types": [
      "node",
      "obsidian"
    ]
  },
  "include": [
    "src/**/*.ts"
  ]
}
